    "rimraf": "^5.0.5",
    "semantic-release": "^23.0.8",
    "serverless": "^4.0.24",
    "serverless-offline": "^13.3.4",
    "serverless-webpack": "^5.13.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.1",
    "wait-on": "^7.2.0",
    "webpack": "^5.91.0"
