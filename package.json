    "check-style": "prettier --check .",
    "fix-style": "prettier --write .",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
