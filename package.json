  "files": [
    "lib"
  ],
  "main": "./lib/index.js",
  "keywords": [
    "middy",
    "middleware",
    "validation",
    "validator"
  ],
  "types": "lib/index.d.ts",
  "engines": {
    "npm": ">= 4.0.0"
  },
  "private": false,
  "dependencies": {
    "@types/debug": ">=4.1.0",
    "class-transformer": ">=0.2.3",
